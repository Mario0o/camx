<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:start ="5dp"
        android:end   ="5dp"
        android:top   ="5dp"
        android:bottom="5dp">
        <shape android:shape="oval">
            <size android:height="22dp"
                android:width   ="22dp" />
            <solid android:color="@color/md3_accent2_100" />
        </shape>
    </item>

    <item>
        <shape
            android:innerRadiusRatio="2.8"
            android:shape="ring"
            android:thickness="2.5dp"
            android:useLevel="false">
            <size android:height="36dp"
                android:width="36dp" />
            <solid android:color="#00000000" />
            <corners android:radius="50dp" />
        </shape>
    </item>
</layer-list>